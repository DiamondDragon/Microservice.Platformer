    <section name="AwsSettingsConfiguration" type=" IntelliFlo.Platform.Config.AwsSettingsConfiguration, IntelliFlo.Platform.NetCore" />
    <section name="autofac" type="Autofac.Configuration.SectionHandler, Autofac.Configuration"/>
