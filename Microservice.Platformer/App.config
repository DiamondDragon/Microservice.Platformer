  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.1"/>
  </startup>
  <autofac>
      <modules>
          <module type="IntelliFlo.Platform.NHibernate.NHibernateConfigurationModule, IntelliFlo.Platform.MicroService">
            <parameters>
              <parameter name="readConnectionStringName" value="master"/>
              <parameter name="writeConnectionStringName" value="master"/>
            </parameters>
          </module>
      </modules>
  </autofac>
  <connectionStrings>
    <clear/>
    <add name="master" connectionString="server=(local);database=Administration;Trusted_Connection=True;"/>
  </connectionStrings>
  <clientConfiguration>
    <serviceEndpoints>
      <add name="crm" baseAddress="http://localhost:10085"/>
      <add name="warehouse" baseAddress="http://localhost:10089"/>
      <add name="account" baseAddress="http://localhost:10102"/>
      <add name="portfolio" baseAddress="http://localhost:10087"/>
    </serviceEndpoints>
  </clientConfiguration>
  <enyim.com>
    <memcached>
      <servers>
        <add address="10.140.2.128" port="11211"/>
      </servers>
      <socketPool minPoolSize="10" maxPoolSize="100" connectionTimeout="00:00:10" deadTimeout="00:02:00"/>
    </memcached>
  </enyim.com>
  <ResourceServerIdentityConfiguration IssuerName="https://idsrv3.com" SigningCertificateSubject="idsrv3test" IdentityServerBaseUrl="https://localhost:44333"/>
  <!-- Configures trusted client authentication. Sets custom token validity period. Section is optional, and default is 30 mins -->
  <TrustedClientAuthConfiguration ValidityPeriodInMinutes="30"/>
  <!-- AWS SDK config. Defines how we authorize to AWS  -->
  <!--Profile location not used in dev, only when microservice is deployed. Please create your AWS profile via VS toolkit as usual-->
  <!--profilesLocation="C:\aws_service_credentials\credentials"-->
  <aws profileName="dev"/>
  <!-- Microservice settings for specifying supported AWS regions -->
  <AwsSettingsConfiguration AwsRegions="eu-west-1"/>
  <common>
    <logging>
      <factoryAdapter type="Common.Logging.Log4Net.Log4NetLoggerFactoryAdapter, Common.Logging.Log4Net1211">
        <arg key="configType" value="INLINE"/>
      </factoryAdapter>
      <!--<factoryAdapter type="Common.Logging.Simple.ConsoleOutLoggerFactoryAdapter, Common.Logging">
        <arg key="level" value="DEBUG" />
        <arg key="showLogName" value="true" />
        <arg key="showDataTime" value="true" />
        <arg key="dateTimeFormat" value="yyyy/MM/dd HH:mm:ss:fff" />
      </factoryAdapter>-->
    </logging>
  </common>
  <log4net debug="false">
    <appender name="ColoredConsoleAppender" type="log4net.Appender.ManagedColoredConsoleAppender">
      <mapping>
        <level value="FATAL"/>
        <foreColor value="Red"/>
      </mapping>
      <mapping>
        <level value="ERROR"/>
        <foreColor value="Red"/>
      </mapping>
      <mapping>
        <level value="WARN"/>
        <foreColor value="Yellow"/>
      </mapping>
      <mapping>
        <level value="DEBUG"/>
        <foreColor value="White"/>
      </mapping>
      <mapping>
        <level value="INFO"/>
        <foreColor value="Green"/>
      </mapping>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline"/>
      </layout>
    </appender>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="c:/logs/Monolith.Bulk.log"/>
      <param name="AppendToFile" value="false"/>
      <param name="RollingStyle" value="Size"/>
      <param name="maxSizeRollBackups" value="1"/>
      <param name="maximumFileSize" value="10MB"/>
      <param name="StaticLogFileName" value="true"/>
      <layout type="log4net.Layout.PatternLayout, log4net">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline"/>
      </layout>
    </appender>
    <root>
      <level value="Info"/>
      <appender-ref ref="ColoredConsoleAppender"/>
      <appender-ref ref="RollingFileAppender"/>
    </root>
    <logger name="NHibernate">
      <level value="WARN"/>
    </logger>
  </log4net>
  <appSettings>
    <add key="enableHibernateProfiler" value="true"/>
    <add key="enableHibernateProfilerStackTrace" value="false"/>
    <add key="client.certificate.default.subjectname" value="XYZ Company"/>
    <add key="AWSProfileName" value="dev"/>
    <add key="db.timeout" value="30"/>
    <add key="cache.use" value="nullcache"/>
    <add key="ClientSettingsProvider.ServiceUri" value=""/>
  </appSettings>
  <applicationSettings>
    <Microservice.Platformer.Properties.Settings>
      <setting name="Environment" serializeAs="String">
        <value>dev</value>
      </setting>
      <setting name="Service" serializeAs="String">
        <value>Performer</value>
      </setting>
      <setting name="ConnectionStringName" serializeAs="String">
        <value>master</value>
      </setting>
      <setting name="BaseAddress" serializeAs="String">
        <value>http://localhost:8888</value>
      </setting>
      <setting name="AltBaseAddress" serializeAs="String">
        <value>http://localhost:8888</value>
      </setting>
      <setting name="Instance" serializeAs="String">
        <value>#{machine.name}</value>
      </setting>
      <setting name="apigateway" serializeAs="String">
        <value />
      </setting>
      <setting name="IsFeatureEnabled" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="DatabaseObjectsFolder" serializeAs="String">
        <value>../../../../../database/Microservice.Performer.SqlServer/dbo</value>
      </setting>
      <setting name="DataScriptFolder" serializeAs="String">
        <value>../../../../../database/Microservice.Performer.SqlServer/dbodata/data</value>
      </setting>
    </Microservice.Platformer.Properties.Settings>
  </applicationSettings>
